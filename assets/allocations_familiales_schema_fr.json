          "anyOf": [
            {
              "type": "string",
              "enum": ["Guadeloupe"],
              "title": "Guadeloupe"
            },
            {
              "type": "string",
              "enum": ["Guyane"],
              "title": "Guyane"
            },
            {
              "type": "string",
              "enum": ["Martinique"],
              "title": "Martinique"
            },
            {
              "type": "string",
              "enum": ["LaReunion"],
              "title": "La Réunion"
            },
            {
              "type": "string",
              "enum": ["SaintBarthelemy"],
              "title": "Saint Barthélemy"
            },
            {
              "type": "string",
              "enum": ["SaintMartin"],
              "title": "Saint Martin"
            },
            {
              "type": "string",
              "enum": ["Metropole"],
              "title": "Métropole"
            },
            {
              "type": "string",
              "enum": ["SaintPierreEtMiquelon"],
              "title": "Saint Pierre et Miquelon"
            },
            {
              "type": "string",
              "enum": ["Mayotte"],
              "title": "Mayotte"
            }
          ]
        },
        "payload": {
          "title": " ",
          "type": "null",
          "default": null
        }
      }
